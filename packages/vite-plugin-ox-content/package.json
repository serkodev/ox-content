{
  "name": "vite-plugin-ox-content",
  "version": "0.0.0",
  "description": "Vite plugin for Ox Content - High-performance Markdown processing with Environment API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@ox-content/napi": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "markdown",
    "ox-content",
    "ssg",
    "environment-api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/ox-content.git",
    "directory": "packages/vite-plugin-ox-content"
  }
}
